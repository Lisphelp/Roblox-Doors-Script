local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Lis Phelps's doors hub", HidePremium = false, SaveConfig = true, ConfigFolder = "LisPhelpsconfiguration", IntroEnabled = true, IntroText = "Lis Phelps's Hub"})

local Tab = Window:MakeTab({
	Name = "Entitys",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
	Name = "Delete Seek",
	Default = false,
	Callback = game.workspace.SeekMoving:delete()
	end    
})

Tab:AddSlider({
	Name = "WalkSpeed",
	Min = 1,
	Max = 21,
	Default = 15,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "walkspeed",
	Callback = function(Value)
		print(Value)
	end    
})

OrionLib:Init()